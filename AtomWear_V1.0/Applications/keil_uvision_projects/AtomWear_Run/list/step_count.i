                            spe_num = 0;
                            Thr_Up = ( ( peak[0].value + peak[1].value + peak[2].value + peak[3].value + peak[4].value)/5 + 1212)/2;
                            Thr_Down = ( ( valley[0].value + valley[1].value + valley[2].value + valley[3].value + valley[4].value)/5 + 909)/2;
                            Thr_Wide = ( ( (peak[0].value - valley[0].value) + (peak[1].value - valley[1].value) + (peak[2].value - valley[2].value) + (peak[3].value - valley[3].value) + (peak[4].value - valley[4].value) )/5 + 707)/2;


                            peak[0].value = peak[1].value = peak[2].value = peak[3].value = peak[4].value = 1010;
                            valley[0].value = valley[1].value = valley[2].value = valley[3].value = valley[4].value =1010;
                            peak[0].time = peak[1].time = peak[2].time = peak[3].time = peak[4].time = 0;
                            valley[0].time = valley[1].time = valley[2].time = valley[3].time = valley[4].time = 0;

                            que_value[0] = que_value[1] = que_value[2] = que_value[3] = que_value[4]  = cur.value = pre.value = 1010;
                            cur.time = pre.time = 0;

                        }
                        else
                        {
                            spe_num++;
                            peak[spe_num].value = valley[spe_num].value = 1212;
                            peak[spe_num].time = valley[spe_num].value = 909;

                            que_value[0] = que_value[1] = que_value[2] = que_value[3] = que_value[4]  = cur.value = pre.value = 1010;
                            cur.time = pre.time = 0;

                        }
                    }
                    else 
                    {
                        pre.value = cur.value;
