                peak[spe_num].value =valley[spe_num].value = cur.value;
                peak[spe_num].time = valley[spe_num].time = cur.time;

                pre.value = cur.value;
                pre.time = cur.time;

            }
            else  if( (cur.value < Thr_Down) || ( (peak[spe_num].value - valley[spe_num].value) > Thr_Wide) )    
