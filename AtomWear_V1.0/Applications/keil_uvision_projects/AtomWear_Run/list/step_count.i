unsigned short CRC_CHECK(unsigned char *Buf, unsigned char CRC_CNT)
{
    unsigned short CRC_Temp;
    unsigned char i,j;
    CRC_Temp = 0xffff;

    for (i=0; i<CRC_CNT; i++)
    {
        CRC_Temp ^= Buf[i];
        for (j=0; j<8; j++)
        {
            if (CRC_Temp & 0x01)
                CRC_Temp = (CRC_Temp >>1 ) ^ 0xa001;
            else
                CRC_Temp = CRC_Temp >> 1;
        }
    }
    return(CRC_Temp);
}

 
int16_t byte_to_word(uint8_t LSB, uint8_t MSB)
{
     
    int16_t acce;
    acce = MSB;
    acce = acce<<6;	 
    acce += ( LSB>>2 );	 

    
    if( acce & 0x2000 )
    {
        acce = ~acce;
        acce &= ~0xc000;     
        acce++;
        g_sum += acce*acce;       
        acce = -acce;
        return acce;	   
    }
    else
    {
        g_sum += acce*acce;
        return acce;
    }
}






uint16_t bma180_get_mo_value()
{
    uint8_t rec[6],i;
    uint16_t acce;
    uint32_t m_acce = 0;

    
    for(i=0; i<3; i++)
    {
        acce = rec[2*i+1];
        acce = acce<<6;	 
        acce += ( rec[2*i]>>2 );

        
        if( acce & 0x2000 )
        {
            acce = ~acce;
            acce &= ~0xc000;     
            acce++;
        }
        m_acce += acce*acce;
    }
    return (uint16_t)sqrt(m_acce);
}






